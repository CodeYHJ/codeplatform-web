  "scripts": {
    "dll": "cross-env NODE_ENV=development webpack --config ./config/webpack.config.dll.js --mode=development",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.config.dev.js --mode=development --port 8080 --hot",
    "build:pretest": "cross-env NODE_ENV=production DEPLOY=pre webpack --mode=production --config ./config/webpack.config.pro.js",
    "build:pro": "cross-env NODE_ENV=production DEPLOY=pro webpack --mode=production --config ./config/webpack.config.pro.js",
    "commit": "git add . && commit && git push",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
