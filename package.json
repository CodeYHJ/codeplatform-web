  "scripts": {
    "cross-dev": "cross-env NODE_ENV=development TS_NODE_PROJECT=\"config/webpack/tsconfig-for-webpack-config.json\"",
    "cross-pro": "cross-env NODE_ENV=production DEPLOY=pro TS_NODE_PROJECT=\"config/webpack/tsconfig-for-webpack-config.json\"",
    "cross-pre": "cross-env NODE_ENV=production DEPLOY=pre TS_NODE_PROJECT=\"config/webpack/tsconfig-for-webpack-config.json\"",
    "dev": "yarn cross-dev webpack-dev-server --config ./config/webpack/webpack.config.dev.ts --hot",
    "build:pretest": "yarn cross-pre webpack --config ./config/webpack/webpack.config.pro.ts",
    "build:pro": "yarn cross-pro --config ./config/webpack/webpack.config.pro.ts",
    "commit": "git add . && commit && git push",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
